{
  "name": "nest-next-example",
  "version": "0.0.0",
  "description": "Example for adding NextJS to NestJS",
  "author": "Kelvin Mai",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@nestjs/common": "latest",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "latest",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/typeorm": "latest",
    "@nestpress/next": "latest",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/passport-jwt": "^3.0.5",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "nest-next": "^9.4.0",
    "next": "^11.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "prop-types": "^15.7.2",
    "react": "latest",
    "react-dom": "latest",
    "react-google-charts": "latest",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^7",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/testing": "latest",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/next": "^8.0.1",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
